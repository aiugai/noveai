{
  "name": "@ai/backend",
  "version": "0.2.6-beta.3",
  "private": true,
  "description": "NestJS application",
  "author": "",
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "build": "pnpm run prisma:generate && nest build && pnpm run postbuild",
    "postbuild": "cp package.production.json ../../dist/backend/package.json",
    "format": "prettier --write \"src/**/*.ts\" --config ../../prettier.config.js",
    "start": "NODE_NO_WARNINGS=1 node -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_NO_WARNINGS=1 nodemon --signal SIGTERM --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'node -r ts-node/register -r tsconfig-paths/register' src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ../../dist/backend/apps/backend/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --no-error-on-unmatched-pattern --config ../../eslint.config.js",
    "test": "jest --config ../../jest.config.js",
    "test:watch": "jest --watch --config ../../jest.config.js",
    "test:cov": "jest --coverage --config ../../jest.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ../../jest.config.js",
    "test:e2e": "cross-env APP_ENV=e2e NODE_OPTIONS=--experimental-vm-modules jest --config e2e/jest-e2e.json --forceExit --detectOpenHandles",
    "test:e2e:file": "cross-env APP_ENV=e2e NODE_OPTIONS=--experimental-vm-modules jest --config e2e/jest-e2e.json --forceExit --detectOpenHandles",
    "test:auth": "jest --config ../../jest.config.js src/modules/auth/tests/auth.service.spec.ts --forceExit --detectOpenHandles",
    "db:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "prisma:generate": "prisma generate --schema=./prisma/schema",
    "prisma:migrate:dev": "prisma migrate dev --schema=./prisma/schema",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema",
    "prisma:migrate:reset": "prisma migrate reset --force --schema=./prisma/schema",
    "prisma:db:push": "prisma db push --schema=./prisma/schema --accept-data-loss",
    "prisma:db:pull": "prisma db pull --schema=./prisma/schema",
    "prisma:db:seed": "prisma db seed --schema=./prisma/schema",
    "prisma:studio": "prisma studio --schema=./prisma/schema",
    "prisma:format": "prisma format --schema=./prisma/schema",
    "prisma:validate": "prisma validate --schema=./prisma/schema"
  },
