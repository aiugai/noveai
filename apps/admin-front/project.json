{
  "name": "admin-front",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/admin-front/src",
  "projectType": "application",
  "tags": ["scope:admin", "type:app", "framework:nextjs"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/admin-front"],
      "defaultConfiguration": "production",
      "options": {
        "command": "node ../../scripts/lib/run-with-version-env.js --app admin -- next build && node scripts/post-build.js",
        "cwd": "apps/admin-front"
      },
      "configurations": {
        "development": {
          "command": "node ../../scripts/lib/run-with-version-env.js --app admin -- next build && node scripts/post-build.js",
          "cwd": "apps/admin-front"
        },
        "production": {
          "command": "node ../../scripts/lib/run-with-version-env.js --app admin -- next build && node scripts/post-build.js",
          "cwd": "apps/admin-front"
        }
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev -H 0.0.0.0 -p 3505",
        "cwd": "apps/admin-front"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "next dev -H 0.0.0.0 -p 3505",
        "cwd": "apps/admin-front"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx --yes serve -s ../../dist/admin-front -l 3505",
        "cwd": "apps/admin-front"
      },
      "dependsOn": ["build"]
    },
    "exportDist": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/admin-front"],
      "options": {
        "command": "node ../../scripts/lib/run-with-version-env.js --app admin -- next build && node scripts/post-build.js",
        "cwd": "apps/admin-front"
      }
    },
    "preview:export": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx --yes serve -s ../../dist/admin-front -l 3505",
        "cwd": "apps/admin-front"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/admin-front/**/*.{ts,tsx,js,jsx}"],
        "eslintConfig": "eslint.config.js"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --skipLibCheck --incremental false",
        "cwd": "apps/admin-front"
      }
    }
  }
}
